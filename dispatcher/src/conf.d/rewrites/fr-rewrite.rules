#
# This file contains the rewrite rules, and can be customized.
#
# By default, it includes just the rewrite rules. You can
# add rewrite rules to this file but you should still include
# the default rewrite rules.

Include conf.d/rewrites/default_rewrite.rules

# rewrite for root redirect
RewriteRule ^/?$ /fr/home.html [L,R=301,NE]

# rewrite for removing trailing slash
RewriteRule ^/(.*)/$ $1 [L,R=301,NE]

# Remove /content/lebara from URL
RewriteCond %{REQUEST_URI} ^/content/lebara
RewriteCond %{REQUEST_URI} (.html)$
RewriteRule ^/content/lebara/fr(.*)$ $1 [L,R=301,NE]

#rewrites request like lebara.fr/abc to lebara.fr/abc/home.html , needed for lebara.fr/fr
RewriteRule ^/([a-z]+)$	/$1/home.html [L,R=301]

RewriteRule ^/([a-z]+)/forfait-tout-en-un-1000-min$ /$1/forfait-mensuel.html [L,R=301]
RewriteRule ^/([a-z]+)/forfait-tout-en-un-illimite$ /$1/forfait-mensuel.html [L,R=301]
RewriteRule ^/([a-z]+)/forfait-mini$ /$1/forfait-mensuel/330122.html [L,R=301]
RewriteRule ^/([a-z]+)/assets/images/8846997028894/1608026254493/151220-lmfr-fup-jan2021forfaits-1-.pdf$ /$1/tarifs.html [L,R=301]
RewriteRule ^/([a-z]+)/assets/images/8945384652830/1579081187070/150120$ /$1/tarifs.html [L,R=301]
RewriteRule ^/([a-z]+)/assets/images/8940862079006/1569939416187/040118$ /$1/tarifs.html [L,R=301]
RewriteRule ^/([a-z]+)/assets/images/8998934249502/1648817687703/Lebara-International-Premium-List-for-Website-April-2022.pdf$ /$1/tarifs.html [L,R=301]
RewriteRule ^/([a-z]+)/assets/images/8846997028894/1608026254493/151220-lmfr-fup-jan2021forfaits-1-.pdf$ /$1/tarifs.html [L,R=301]
RewriteRule ^/([a-z]+)/assets/images/8945384652830/1579081187070/150120$ /$1/tarifs.html [L,R=301]
RewriteRule ^/([a-z]+)/assets/images/8940862079006/1569939416187/040118$ /$1/tarifs.html [L,R=301]
RewriteRule ^/([a-z]+)/assets/images/8998934249502/1648817687703/Lebara-International-Premium-List-for-Website-April-2022.pdf$ /$1/tarifs.html [L,R=301]
RewriteRule ^/([a-z]+)/decouvrir-forfait$ /$1/blog/decouvrir-forfait.html [L,R=301]
RewriteRule ^/([a-z]+)/meilleure-offre$ /$1/blog/meilleure-offre.html [L,R=301]
RewriteRule ^/([a-z]+)/question/FaqBlock4QuestionAnswer2$ /$1/faq/changer-forfait.html [L,R=301]
RewriteRule ^/([a-z]+)/question/faqblock5questionanswer3$ /$1/faq/controle-parental.html [L,R=301]
RewriteRule ^/([a-z]+)/question/faq-internet-4g$ /$1/faq/internet-4g.html [L,R=301]
RewriteRule ^/([a-z]+)/question/FaqBlock4QuestionAnswer3$ /$1/faq/offres.html [L,R=301]
RewriteRule ^/([a-z]+)/forfait-sans-engagement/monthly-bundle-80gb-999/p/3300150$ /$1/forfait-mensuel/3300150.html [L,R=301]
RewriteRule ^/([a-z]+)/offrelimitee80go$ /$1/offrelimitee80go.html [L,R=301]
RewriteRule ^/([a-z]+)/prix-des-offres-des-operateurs-de-telephonie$ /$1/prix-des-offres-des-operateurs-de-telephonie.html [L,R=301]
RewriteRule ^/([a-z]+)/revendeur-forfaits-mobile$ /$1/revendeur-forfaits-mobile.html [L,R=301]
RewriteRule ^/([a-z]+)/pass-internet-s$ /$1/pass-internet/3355205.html [L,R=301]
RewriteRule ^/([a-z]+)/pass-internet-s$ /$1/pass-internet/3355205.html [L,R=301]

# Robots rewrite
RewriteRule ^/robots.txt$ /content/dam/${CONTENT_FOLDER_NAME}/markets/fr/robots.txt [NC,PT]

# Sitemap rewrite
RewriteRule ^/sitemap.xml$ /content/${CONTENT_FOLDER_NAME}/fr.sitemap.xml [NC,PT]

# SpeedKit rewrite
RewriteRule ^/wrapper-sw.js$ /etc.clientlibs/lebara/clientlibs/clientlib-speedkit-fr.js [NC,PT]

# Google Merchant Center rewrite
RewriteRule ^/google331b37d915cf40bc.html$ /content/dam/${CONTENT_FOLDER_NAME}/markets/google331b37d915cf40bc.html [NC,PT]

RewriteCond %{REQUEST_URI} !^/apps
RewriteCond %{REQUEST_URI} !^/bin
RewriteCond %{REQUEST_URI} !^/content
RewriteCond %{REQUEST_URI} !^/etc
RewriteCond %{REQUEST_URI} !^/home
RewriteCond %{REQUEST_URI} !^/libs
RewriteCond %{REQUEST_URI} !^/saml_login
RewriteCond %{REQUEST_URI} !^/system
RewriteCond %{REQUEST_URI} !^/tmp
RewriteCond %{REQUEST_URI} !^/var
RewriteCond %{REQUEST_URI} !^/api
RewriteCond %{REQUEST_URI} (.html|.jpe?g|.png|.svg|.json|.xml)$
RewriteRule ^/(.*)$ /content/${CONTENT_FOLDER_NAME}/fr/$1 [PT,L]


# rewrite for root redirect extensionless urls
RewriteCond %{REQUEST_URI} !^/apps
RewriteCond %{REQUEST_URI} !^/bin
RewriteCond %{REQUEST_URI} !^/content
RewriteCond %{REQUEST_URI} !^/etc
RewriteCond %{REQUEST_URI} !^/home
RewriteCond %{REQUEST_URI} !^/libs
RewriteCond %{REQUEST_URI} !^/saml_login
RewriteCond %{REQUEST_URI} !^/system
RewriteCond %{REQUEST_URI} !^/tmp
RewriteCond %{REQUEST_URI} !^/var
RewriteCond %{REQUEST_URI} !^/api
RewriteCond %{REQUEST_URI} !.html$
RewriteRule ^/(.*)$ /content/${CONTENT_FOLDER_NAME}/fr/$1.html [PT,L]