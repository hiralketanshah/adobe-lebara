<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Confirmation Message"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="cmp-navigation__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <confirmation
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Confirmation Mesaage"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <orderidtitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Order N."
                                                fieldLabel="Order Id Title"
                                                name="./orderIdTitle"/>
                                            <simonlytitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Youâ€™re all set!"
                                                fieldLabel="Sim Only Title"
                                                name="./simOnlyTitle"/>
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Thank you. We have received your request for a SIM only plan change."
                                                fieldLabel="Title"
                                                name="./title"/>
                                            <fmvnoConfirmationtitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Thank you!"
                                                fieldLabel="Sim Migration Title"
                                                name="./fmvnoConfirmationtitle"/>
                                            <orderidsubtitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Thank you for purchasing the selected product. Your SIM will arrive next working day. Occasionally it may take a little longer."
                                                fieldLabel="Order Id Subtitle"
                                                name="./orderIdSubtitle"/>
                                            <simonlysubtitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="You can now start calling, texting and using data."
                                                fieldLabel="Sim Only Subtitle"
                                                name="./simOnlySubtitle"/>
                                            <planchangesubtitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="The plan change for the number {0} from the {1} to the {2} will take place on the first day of the following month. You will receive an email with further details about your new SIM only plan shortly."
                                                fieldLabel="Plan Change Subtitle"
                                                name="./planChangeSubtitle"/>
                                            <fmvnoConfirmationSubtitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Your migration request is completed. You will receive a confirmation SMS with your date of migration. The day of the migration, simply insert the new SIM on your phone. All your credit and bundles will be kept."
                                                fieldLabel="Sim Migration Subtitle"
                                                name="./fmvnoConfirmationSubtitle"/>
                                            <dashboardbuttonlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Goto Dashboard"
                                                fieldLabel="Dashboard Button Label"
                                                name="./dashboardButtonLabel"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </confirmation>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
