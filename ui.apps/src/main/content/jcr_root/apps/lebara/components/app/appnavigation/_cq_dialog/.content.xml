<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="App - Navigation"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <navigationLinks
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Navigation Links"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <list
                                                granite:class="lebara-multifield__editor"
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Navigation List"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldDescription="Add the Navigation Links"
                                                fieldLabel="Navigation List">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    maxItems="{Long}15"/>
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./navigationLinks">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <icon
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                    fieldDescription="Choose Image"
                                                                    fieldLabel="Link Icon"
                                                                    name="./icon"
                                                                    rootPath="/content/dam/lebara"/>
                                                                <name
                                                                   jcr:primaryType="nt:unstructured"
                                                                   sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                   fieldDescription="Navigation Title"
                                                                   fieldLabel="Navigation Title"
                                                                   required="true"
                                                                   name="./label"/>
                                                                <link
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                    fieldDescription="Navigation Link"
                                                                    fieldLabel="Navigation Link"
                                                                    name="./appNavigationLink">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <addon
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="add_on"
                                                                            value="add_on"/>
                                                                        <freesim
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="free_sim"
                                                                            value="free_sim"/>
                                                                        <help
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="help"
                                                                            value="help"/>
                                                                        <language
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="language"
                                                                            value="language"/>
                                                                        <more
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="more"
                                                                            value="more"/>
                                                                        <mylebara
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="mylebara"
                                                                            value="mylebara"/>
                                                                        <myprofile
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="my_profile"
                                                                            value="my_profile"/>
                                                                        <managesim
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="manage_sim"
                                                                            value="manage_sim"/>
                                                                        <paymenthistory
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="payment_history"
                                                                            value="payment_history"/>
                                                                        <postpaid
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="postpaid"
                                                                            value="postpaid"/>
                                                                        <prepaid
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="prepaid"
                                                                            value="prepaid"/>
                                                                        <registersim
                                                                            jcr:primaryType="nt:unstructured"    
                                                                            text="register_sim"
                                                                            value="register_sim"/>
                                                                        <savedpayments
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="saved_payments"
                                                                            value="saved_payments"/>
                                                                        <shop
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="shop"
                                                                            value="shop"/>
                                                                        <statements
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="statements"
                                                                            value="statements"/>
                                                                        <topup
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="top_up"
                                                                            value="top_up"/>
                                                                        <usage
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="usage"
                                                                            value="usage"/>
                                                                    </items>
                                                                </link>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </list>
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Close Button Label"
                                                fieldLabel="Close Button Label"
                                                name="./closeButtonLabel"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </navigationLinks>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
