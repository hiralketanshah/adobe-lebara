<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Postpaid Personal Details Component"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <column1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Name Section"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <personalDetailsHeading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Your personal details"
                                fieldLabel="Personal Details Title"
                                name="./frmFields/heading"
                                value="Your personal details"/>
                            <showPersonalDetailsLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./frmFields/showPersonalDetailsLabel"
                                text="Show Personal Details Label"
                                uncheckedValue="{Boolean}false"
                                value="{Boolean}true"/>
                            <titleLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Title"
                                fieldLabel="Title Label"
                                name="./frmFields/titleLabel"
                                value="Title"/>
                            <titlePlaceholder
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Select title"
                                fieldLabel="Title Placeholder"
                                name="./frmFields/titlePlaceholder"
                                value="Select title"/>
                            <showTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./frmFields/showTitle"
                                text="Show Title"
                                uncheckedValue="{Boolean}false"
                                value="{Boolean}true"/>
                            <titleIsFirst
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./frmFields/titleIsFirst"
                                text="Title Is First"
                                uncheckedValue="{Boolean}false"
                                value="{Boolean}true"/>
                            <emailAddressLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Email Address"
                                fieldLabel="Email Address Label"
                                name="./frmFields/emailLabel"
                                value="Email Address"/>
                            <emailAddressLabelPlaceholder
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Your Email Address"
                                fieldLabel="Email Address Placeholder"
                                name="./frmFields/emailPlaceholder"
                                value="Enter Your Email Address"/>
                            <enableEmailCheckbox
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./frmFields/enableEmailCheckbox"
                                text="Enable Email"
                                uncheckedValue="{Boolean}false"
                                value="{Boolean}true"/>
                            <firstNameLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="First Name"
                                fieldLabel="First Name Label"
                                name="./frmFields/fNameLabel"
                                value="First Name"/>
                            <firstNamePlaceholder
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Your First Name"
                                fieldLabel="First Name Placeholder"
                                name="./frmFields/fnamePlaceholder"
                                value="Enter Your First Name"/>
                            <lastNameLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Last Name Label"
                                name="./frmFields/lNameLabel"
                                value="Last Name"/>
                            <lastNamePlaceholder
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Your First Name"
                                fieldLabel="Last Name Placeholder"
                                name="./frmFields/lNamePlaceholder"
                                value="Enter Your First Name"/>
                            <dobtitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="DOB title"
                                name="./frmFields/dobLabel"
                                value="Date of Birth"/>
                            <showDob
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./frmFields/showDob"
                                text="Show Date of birth"
                                uncheckedValue="{Boolean}false"
                                value="{Boolean}true"/>
                            <dayfieldlabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Day Field Label "
                                name="./frmFields/dayLabel"
                                value="Day"/>
                            <dayPalceholder
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Day"
                                fieldLabel="Day Placeholder"
                                name="./frmFields/dayPalceholder"
                                value="Enter Day"/>
                            <monthLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Month Field Label "
                                name="./frmFields/monthLabel"
                                value="Month"/>
                            <monthPlaceholder
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Month"
                                fieldLabel="Month Placeholder"
                                name="./frmFields/monthPlaceholder"
                                value="Enter Month"/>
                            <yearLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Year Field Label "
                                name="./frmFields/yearLabel"
                                value="Year"/>
                            <yearPlaceholder
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Year"
                                fieldLabel="Year Placeholder"
                                name="./frmFields/yearPlaceholder"
                                value="Enter Year"/>
                            <dayOfBirthByFields
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./frmFields/dayOfBirthByFields"
                                text="Day of birth by fields"
                                uncheckedValue="{Boolean}false"
                                value="{Boolean}true"/>
                            <showAddressCard
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./frmFields/showAddressCard"
                                text="Show Address Card"
                                uncheckedValue="{Boolean}false"
                                value="{Boolean}true"/>
                            <cardMode
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./frmFields/cardMode"
                                text="Card Mode"
                                uncheckedValue="{Boolean}false"
                                value="{Boolean}true"/>
                        </items>
                    </column1>
                    <column2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Port Your Number"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <portingSectionHeading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Port Your Number Title"
                                name="./frmFields/portingSectionHeading"
                                value="Would you like to Port your Number?"/>
                            <nothanksLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="No Thanks Radio Label"
                                name="./frmFields/portInOptions/item0/label"
                                value="No, I want a new number"/>
                            <nothanksValue
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                fieldLabel="No Thanks Radio value"
                                name="./frmFields/portInOptions/item0/value"
                                value="No"/>
                            <wantToPortLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Want To Port In Radio Label"
                                name="./frmFields/portInOptions/item1/label"
                                value="Yes, I would like to transfer my number"/>
                            <wantToPortValue
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                fieldLabel="Want To Port In Radio Value"
                                name="./frmFields/portInOptions/item1/value"
                                value="Yes"/>
                            <portInNumberLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Number To Port Label"
                                name="./frmFields/portInNumberLabel"
                                value="Number to ported to Sim only"/>
                            <portInNumberPlaceHolder
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Port In Number PlaceHolder"
                                name="./frmFields/portInNumberPlaceHolder"
                                value="Enter your mobile number"/>
                            <noPortInDescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                fieldLabel="No PortIn Description"
                                name="./frmFields/noPortInDescription"
                                useFixedInlineToolbar="{Boolean}true">
                                <rtePlugins jcr:primaryType="nt:unstructured">
                                    <format
                                        jcr:primaryType="nt:unstructured"
                                        features="bold,italic"/>
                                    <justify
                                        jcr:primaryType="nt:unstructured"
                                        features="-"/>
                                    <links
                                        jcr:primaryType="nt:unstructured"
                                        features="modifylink,unlink"/>
                                    <lists
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <misctools jcr:primaryType="nt:unstructured">
                                        <specialCharsConfig jcr:primaryType="nt:unstructured">
                                            <chars jcr:primaryType="nt:unstructured">
                                                <default_copyright
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;copy;"
                                                    name="copyright"/>
                                                <default_euro
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;euro;"
                                                    name="euro"/>
                                                <default_registered
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;reg;"
                                                    name="registered"/>
                                                <default_trademark
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;trade;"
                                                    name="trademark"/>
                                            </chars>
                                        </specialCharsConfig>
                                    </misctools>
                                    <paraformat
                                        jcr:primaryType="nt:unstructured"
                                        features="*">
                                        <formats jcr:primaryType="nt:unstructured">
                                            <default_p
                                                jcr:primaryType="nt:unstructured"
                                                description="Paragraph"
                                                tag="p"/>
                                            <default_h1
                                                jcr:primaryType="nt:unstructured"
                                                description="Heading 1"
                                                tag="h1"/>
                                            <default_h2
                                                jcr:primaryType="nt:unstructured"
                                                description="Heading 2"
                                                tag="h2"/>
                                            <default_h3
                                                jcr:primaryType="nt:unstructured"
                                                description="Heading 3"
                                                tag="h3"/>
                                            <default_h4
                                                jcr:primaryType="nt:unstructured"
                                                description="Heading 4"
                                                tag="h4"/>
                                            <default_h5
                                                jcr:primaryType="nt:unstructured"
                                                description="Heading 5"
                                                tag="h5"/>
                                            <default_h6
                                                jcr:primaryType="nt:unstructured"
                                                description="Heading 6"
                                                tag="h6"/>
                                            <default_blockquote
                                                jcr:primaryType="nt:unstructured"
                                                description="Quote"
                                                tag="blockquote"/>
                                            <default_pre
                                                jcr:primaryType="nt:unstructured"
                                                description="Preformatted"
                                                tag="pre"/>
                                        </formats>
                                    </paraformat>
                                    <table
                                        jcr:primaryType="nt:unstructured"
                                        features="-">
                                        <hiddenHeaderConfig
                                            jcr:primaryType="nt:unstructured"
                                            hiddenHeaderClassName="cq-wcm-foundation-aria-visuallyhidden"
                                            hiddenHeaderEditingCSS="cq-RichText-hiddenHeader--editing"/>
                                    </table>
                                    <tracklinks
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                </rtePlugins>
                                <uiSettings jcr:primaryType="nt:unstructured">
                                    <cui jcr:primaryType="nt:unstructured">
                                        <inline
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[format#bold,format#italic,format#underline,#justify,#lists,links#modifylink,links#unlink,#paraformat]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <justify
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                    ref="justify"/>
                                                <lists
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                    ref="lists"/>
                                                <paraformat
                                                    jcr:primaryType="nt:unstructured"
                                                    items="paraformat:getFormats:paraformat-pulldown"
                                                    ref="paraformat"/>
                                            </popovers>
                                        </inline>
                                        <dialogFullScreen
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[format#bold,format#italic,format#underline,justify#justifyleft,justify#justifycenter,justify#justifyright,lists#unordered,lists#ordered,lists#outdent,lists#indent,links#modifylink,links#unlink,table#createoredit,#paraformat,image#imageProps,experience-aem#structuredContentModal]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <paraformat
                                                    jcr:primaryType="nt:unstructured"
                                                    items="paraformat:getFormats:paraformat-pulldown"
                                                    ref="paraformat"/>
                                            </popovers>
                                        </dialogFullScreen>
                                        <tableEditOptions
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[table#insertcolumn-before,table#insertcolumn-after,table#removecolumn,-,table#insertrow-before,table#insertrow-after,table#removerow,-,table#mergecells-right,table#mergecells-down,table#mergecells,table#splitcell-horizontal,table#splitcell-vertical,-,table#selectrow,table#selectcolumn,-,table#ensureparagraph,-,table#modifytableandcell,table#removetable,-,undo#undo,undo#redo,-,table#exitTableEditing,-]"/>
                                    </cui>
                                </uiSettings>
                            </noPortInDescription>
                            <yesPortInDescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                fieldLabel="Yes PortIn Description"
                                name="./frmFields/yesPortInDescription"
                                useFixedInlineToolbar="{Boolean}true">
                                <rtePlugins jcr:primaryType="nt:unstructured">
                                    <format
                                        jcr:primaryType="nt:unstructured"
                                        features="bold,italic"/>
                                    <justify
                                        jcr:primaryType="nt:unstructured"
                                        features="-"/>
                                    <links
                                        jcr:primaryType="nt:unstructured"
                                        features="modifylink,unlink"/>
                                    <lists
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <misctools jcr:primaryType="nt:unstructured">
                                        <specialCharsConfig jcr:primaryType="nt:unstructured">
                                            <chars jcr:primaryType="nt:unstructured">
                                                <default_copyright
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;copy;"
                                                    name="copyright"/>
                                                <default_euro
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;euro;"
                                                    name="euro"/>
                                                <default_registered
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;reg;"
                                                    name="registered"/>
                                                <default_trademark
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;trade;"
                                                    name="trademark"/>
                                            </chars>
                                        </specialCharsConfig>
                                    </misctools>
                                    <paraformat
                                        jcr:primaryType="nt:unstructured"
                                        features="*">
                                        <formats jcr:primaryType="nt:unstructured">
                                            <default_p
                                                jcr:primaryType="nt:unstructured"
                                                description="Paragraph"
                                                tag="p"/>
                                            <default_h1
                                                jcr:primaryType="nt:unstructured"
                                                description="Heading 1"
                                                tag="h1"/>
                                            <default_h2
                                                jcr:primaryType="nt:unstructured"
                                                description="Heading 2"
                                                tag="h2"/>
                                            <default_h3
                                                jcr:primaryType="nt:unstructured"
                                                description="Heading 3"
                                                tag="h3"/>
                                            <default_h4
                                                jcr:primaryType="nt:unstructured"
                                                description="Heading 4"
                                                tag="h4"/>
                                            <default_h5
                                                jcr:primaryType="nt:unstructured"
                                                description="Heading 5"
                                                tag="h5"/>
                                            <default_h6
                                                jcr:primaryType="nt:unstructured"
                                                description="Heading 6"
                                                tag="h6"/>
                                            <default_blockquote
                                                jcr:primaryType="nt:unstructured"
                                                description="Quote"
                                                tag="blockquote"/>
                                            <default_pre
                                                jcr:primaryType="nt:unstructured"
                                                description="Preformatted"
                                                tag="pre"/>
                                        </formats>
                                    </paraformat>
                                    <table
                                        jcr:primaryType="nt:unstructured"
                                        features="-">
                                        <hiddenHeaderConfig
                                            jcr:primaryType="nt:unstructured"
                                            hiddenHeaderClassName="cq-wcm-foundation-aria-visuallyhidden"
                                            hiddenHeaderEditingCSS="cq-RichText-hiddenHeader--editing"/>
                                    </table>
                                    <tracklinks
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                </rtePlugins>
                                <uiSettings jcr:primaryType="nt:unstructured">
                                    <cui jcr:primaryType="nt:unstructured">
                                        <inline
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[format#bold,format#italic,format#underline,#justify,#lists,links#modifylink,links#unlink,#paraformat]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <justify
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                    ref="justify"/>
                                                <lists
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                    ref="lists"/>
                                                <paraformat
                                                    jcr:primaryType="nt:unstructured"
                                                    items="paraformat:getFormats:paraformat-pulldown"
                                                    ref="paraformat"/>
                                            </popovers>
                                        </inline>
                                        <dialogFullScreen
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[format#bold,format#italic,format#underline,justify#justifyleft,justify#justifycenter,justify#justifyright,lists#unordered,lists#ordered,lists#outdent,lists#indent,links#modifylink,links#unlink,table#createoredit,#paraformat,image#imageProps,experience-aem#structuredContentModal]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <paraformat
                                                    jcr:primaryType="nt:unstructured"
                                                    items="paraformat:getFormats:paraformat-pulldown"
                                                    ref="paraformat"/>
                                            </popovers>
                                        </dialogFullScreen>
                                        <tableEditOptions
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[table#insertcolumn-before,table#insertcolumn-after,table#removecolumn,-,table#insertrow-before,table#insertrow-after,table#removerow,-,table#mergecells-right,table#mergecells-down,table#mergecells,table#splitcell-horizontal,table#splitcell-vertical,-,table#selectrow,table#selectcolumn,-,table#ensureparagraph,-,table#modifytableandcell,table#removetable,-,undo#undo,undo#redo,-,table#exitTableEditing,-]"/>
                                    </cui>
                                </uiSettings>
                            </yesPortInDescription>
                            <ctaContinueLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Continue CTA Label"
                                name="./frmFields/ctaContinueLabel"
                                value="Continue"/>
                            <showContinueButton
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./frmFields/showContinueButton"
                                text="Show Continue Button"
                                uncheckedValue="{Boolean}false"
                                value="{Boolean}true"/>
                        </items>
                    </column2>
                    <column3
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Error Messages"
                        sling:resourceType="granite/ui/components/coral/foundation/include"
                        path="/apps/lebara/components/generic-tabs/errorMessages/cq:dialog/content/items/tabs/items/column"/>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
