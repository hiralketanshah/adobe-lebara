<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="OTAC Verification"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[lebara.touchui]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <formfields
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Form Fields"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <otacVerifyLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Verify with OTAC"
                                                fieldLabel="OTAC Verification Label"
                                                name="./otacVerifyLabel"
                                                value="Verify with OTAC"/>
                                            <otacVerifyDesc
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldDescription="To continue you need to put verification code."
                                                fieldLabel="OTAC verification  Description"
                                                name="./otacVerifyDesc"/>
                                            <mobileLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Mobile Number"
                                                fieldLabel="Mobile Number Label"
                                                name="./mobileLabel"
                                                value="Mobile Number"/>
                                            <mobileDesc
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="ENter your X digits mobile number"
                                                fieldLabel="Mobile Number Description"
                                                name="./mobileDesc"
                                                value="ENter your X digits mobile number"/>
                                            <ctaContinueLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Continue"
                                                fieldLabel="Continue Button Label"
                                                name="./ctaContinueLabel"
                                                value="Continue"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </formfields>
                    <verificationlabels
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Error Messages"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <continueVerifyLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Verify to continue"
                                                fieldLabel="Continue Verify Label"
                                                name="./continueVerifyLabel"
                                                value="Verify to continue"/>
                                            <continueVerifyDesc
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldDescription="To continue change of address get the verification address and put it below/"
                                                fieldLabel="OTAC verification  Description"
                                                name="./continueVerifyDesc"/>
                                            <enterCodeLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the code sent to {0}"
                                                fieldLabel="Enter code Label"
                                                name="./enterCodeLabel"
                                                value="Enter the code sent to {0}"/>
                                            <simCardLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Sim card number"
                                                fieldLabel="Sim card number Label"
                                                name="./simCardLabel"
                                                value="Sim card number"/>
                                            <verifyCodeLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Verification Code"
                                                fieldLabel="Verification Code Label"
                                                name="./verifyCodeLabel"
                                                value="Verification Code"/>
                                            <verifyCodePlaceholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Verification code"
                                                fieldLabel="Verification Code Label"
                                                name="./verifyCodePlaceholder"
                                                value="Enter Verification code"/>
                                            <resendVerifyCodeInLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Resend verification code in"
                                                fieldLabel="Resend verification code in Label"
                                                name="./resendVerifyCodeInLabel"
                                                value="Resend verification code in"/>
                                            <resendVerifyCodeLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Resend verification code"
                                                fieldLabel="Resend verification code Label"
                                                name="./resendVerifyCodeLabel"
                                                value="Resend verification code"/>
                                            <verifyCodeInValidMsg
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Verification code is incorrect"
                                                fieldLabel="Verification Code Invalid Error Message"
                                                name="./verifyCodeInvalidMsg"
                                                value="Verification code is incorrect"/>
                                            <exceededLimitLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Exceeded the limit"
                                                fieldLabel="Exceeded Limit Label"
                                                name="./exceededLimitLabel"
                                                value="Exceeded the limit"/>
                                            <exceededLimitDesc
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldDescription="You have exceeded the code limit entry in ( 3 times in 60 secs). Verify with OTAC is blocked to 30 mins"
                                                fieldLabel="Exceeded Limit Description"
                                                name="./exceededLimitDesc"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </verificationlabels>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
