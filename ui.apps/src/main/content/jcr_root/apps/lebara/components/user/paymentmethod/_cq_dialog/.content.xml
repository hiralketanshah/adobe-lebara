<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Payment Method"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="cmp-navigation__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <preferredPaymentLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Preferred Payment Label"
                                                name="./preferredPaymentLabel"
                                                value="Prefered Payment"/>
                                            <preferredPaymentDesc
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Preferred Payment Description"
                                                name="./preferredPaymentDesc"
                                                value="Your prefered payment method is not set yet.If already requested kindly wait for our confirmation."/>
                                            <defaultPaymentLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Default Payment Label"
                                                name="./defaultPaymentLabel"
                                                value="Default Payment"/>
                                            <otherSavedPaymentMethodLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Other Saved Payment Method Label"
                                                name="./otherSavedPaymentMethodLabel"
                                                value="Other Saved Payment Method"/>
                                            <paymentMethodsLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Payment Methods Label"
                                                name="./paymentMethodsLabel"
                                                value="Payment Methods "/>
                                            <howItWorksLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="How It Works Label"
                                                name="./howItWorksLabel"
                                                value="How it works?"/>
                                            <howItWorksDesc
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldLabel="How It Works Description"
                                                name="./howItWorksDesc"
                                                value="You are now setting up your preferred payment method and enabling a continuous payment authority for Lebara. This is a highly convenient payment set-up that means you no longer need to enter payment method info for each and every different Lebara purchase or payment you make. One payment method for all purchases and recurring payments, set-up one time only, simple!"/>
                                            <creditOrDeditCardLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Credit or Debit card Label"
                                                name="./creditOrDeditCardLabel"
                                                value="Add Credit / Debit Card"/>
                                            <payWithPaypalLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Pay with Paypal Label"
                                                name="./payWithPaypalLabel"
                                                value="Pay with PayPal"/>
                                            <setDefaultLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Set Default Label"
                                                name="./setDefaultLabel"
                                                value="Set Default"/>
                                            <deleteLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Delete Label"
                                                name="./deleteLabel"
                                                value="Delete"/>
                                            <hideKlarnaPay
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                text="Hide Klarna Pay?"
                                                name="./hideKlarnaPay"
                                                uncheckedValue="{Boolean}false"
                                                value="{Boolean}true"/>
                                            <hideGiroPay
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                text="Hide Giro Pay?"
                                                name="./hideGiroPay"
                                                uncheckedValue="{Boolean}false"
                                                value="{Boolean}true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </general>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
