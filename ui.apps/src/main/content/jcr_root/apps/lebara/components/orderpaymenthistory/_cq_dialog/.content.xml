                                value="Order and Payment History"/>
                            <historyLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="History Label"
                                fieldLabel="History Label"
                                name="./frmFields/historyLabel"
                                value="History"/>
                            <numberHistoryLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Number History Label"
                                fieldLabel="Number History Label"
                                name="./frmFields/numberHistoryLabel"
                                value="015171918893"/>
                            <firstNameLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="First Name"
                                fieldLabel="First Name Label"
                                name="./frmFields/fNameLabel"
                                value="First Name"/>
                            <firstNamePlaceholder
                                jcr:primaryType="nt:unstructured"
