                        jcr:title="Modal Data"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <keyInAddress
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Key in your address manually"
                                    value="Key in your address manually"
                                    fieldDescription="Key in your address manually"
                                    name="./frmFields/keyInAddress"/>
                            <addressKeyInText
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Address key in message"
                                    value="Key in your postal code and find your address"
                                    fieldDescription="Key in your postal code and find your address"
                                    name="./frmFields/addressKeyInText"/>
                            <postalcodePlaceholder
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Search For Postal Code placeholder text"
                                    value="Search For Postal Code"
                                    fieldDescription="Search For Postal Code"
                                    name="./frmFields/postalcodePlaceholder"/>
