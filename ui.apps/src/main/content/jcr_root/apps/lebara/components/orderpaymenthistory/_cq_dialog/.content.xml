                                    fieldLabel="Save address for future label"
                                    value="Save address for future"
                                    fieldDescription="Save address for future"
                                    name="./frmFields/saveAddress"/>
                            <shippingLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Shipping Address"
                                fieldLabel="Address label"
                                name="./frmFields/shippingLabel"
