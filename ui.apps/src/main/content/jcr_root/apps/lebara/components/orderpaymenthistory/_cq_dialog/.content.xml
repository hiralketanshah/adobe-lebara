                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Shipping Helper Text"
                                fieldLabel="Shipping Helper Text"
                                name="./frmFields/shippingHelperText"
                                value="Key in your postal code and find your address"/>
                            <consentPreviewText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                fieldDescription="Consent Preview Text"
                                fieldLabel="Consent Preview Text"
                                name="./frmFields/consentPreviewText"
                                value="I consent to trustworthy cooperation partners of Lebara contacting me about their products and services for"/>

