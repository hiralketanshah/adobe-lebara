<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Footer component"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[lebara.touchui]">
    <content
        granite:class="cmp-navigation__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <column1
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Column 1"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <well
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <pages
                                                            granite:class="foundation-layout-util-maximized-alt coral-Form-fieldwrapper"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <field
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="cq/gui/components/coral/common/form/pagefield"
                                                                    fieldLabel="Parent Link"
                                                                    name="./column1Link"
                                                                    rootPath="/content/lebara"/>
                                                            <multi
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                    composite="{Boolean}true"
                                                                    fieldLabel="Child Links">
                                                                <field
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                        name="./column1">
                                                                    <granite:data
                                                                            jcr:primaryType="nt:unstructured"/>
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <column
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                <pagepath
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        sling:resourceType="cq/gui/components/coral/common/form/pagefield"
                                                                                        name="./link"
                                                                                        rootPath="/content/lebara"/>
                                                                            </items>
                                                                        </column>
                                                                    </items>
                                                                </field>
                                                            </multi>
                                                        </items>
                                                    </pages>
                                                </items>
                                            </well>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </column1>
                    <column2
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Column 2"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <well
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <pages
                                                            granite:class="foundation-layout-util-maximized-alt coral-Form-fieldwrapper"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <field
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="cq/gui/components/coral/common/form/pagefield"
                                                                    fieldLabel="Parent Link"
                                                                    name="./column2Link"
                                                                    rootPath="/content/lebara"/>
                                                            <multi
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                    composite="{Boolean}true"
                                                                    fieldLabel="Child Links">
                                                                <field
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                        name="./column2">
                                                                    <granite:data
                                                                            jcr:primaryType="nt:unstructured"/>
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <column
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                <pagepath
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        sling:resourceType="cq/gui/components/coral/common/form/pagefield"
                                                                                        name="./link"
                                                                                        rootPath="/content/lebara"/>
                                                                            </items>
                                                                        </column>
                                                                    </items>
                                                                </field>
                                                            </multi>
                                                        </items>
                                                    </pages>
                                                </items>
                                            </well>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </column2>
                    <column3
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Column3"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <well
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <pages
                                                            granite:class="foundation-layout-util-maximized-alt coral-Form-fieldwrapper"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <field
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="cq/gui/components/coral/common/form/pagefield"
                                                                    fieldLabel="Parent Link"
                                                                    name="./column3Link"
                                                                    rootPath="/content/lebara"/>
                                                            <multi
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                    composite="{Boolean}true"
                                                                    fieldLabel="Child Links">
                                                                <field
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                        name="./column3">
                                                                    <granite:data
                                                                            jcr:primaryType="nt:unstructured"/>
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <column
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                <pagepath
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        sling:resourceType="cq/gui/components/coral/common/form/pagefield"
                                                                                        name="./link"
                                                                                        rootPath="/content/lebara"/>
                                                                            </items>
                                                                        </column>
                                                                    </items>
                                                                </field>
                                                            </multi>
                                                        </items>
                                                    </pages>
                                                </items>
                                            </well>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </column3>
                    <column4
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Column 4"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <well
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <pages
                                                            granite:class="foundation-layout-util-maximized-alt coral-Form-fieldwrapper"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <field
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="cq/gui/components/coral/common/form/pagefield"
                                                                    fieldLabel="Parent Link"
                                                                    name="./column4Link"
                                                                    rootPath="/content/lebara"/>
                                                            <multi
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                    composite="{Boolean}true"
                                                                    fieldLabel="Child Links">
                                                                <field
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                        name="./column4">
                                                                    <granite:data
                                                                            jcr:primaryType="nt:unstructured"/>
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <column
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                <pagepath
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        sling:resourceType="cq/gui/components/coral/common/form/pagefield"
                                                                                        name="./link"
                                                                                        rootPath="/content/lebara"/>
                                                                            </items>
                                                                        </column>
                                                                    </items>
                                                                </field>
                                                            </multi>
                                                        </items>
                                                    </pages>
                                                </items>
                                            </well>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </column4>
                    <additionalLinks
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Footer Copyright Section"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <copyrighttext
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Copyright Text"
                                                fieldLabel="Copyright Text"
                                                name="./footercopyright/copyrightText"/>
                                            <footercopyright
                                                granite:class="lebara-multifield__editor"
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Plans"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldDescription="Add the footer copyright section"
                                                fieldLabel="Footer">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    maxItems="{Long}3"/>
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./footercopyright/links">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <label
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Enter Label"
                                                                    fieldLabel="Link label"
                                                                    name="./label"/>
                                                                <labellink
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                    fieldDescription="Enter Label link"
                                                                    fieldLabel="Link"
                                                                    name="./link"
                                                                    rootPath="/content/lebara"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </footercopyright>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </additionalLinks>
                    <getApp
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Get App Section"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true"
                        name="./getApp">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        name="./getApp1">
                                        <items jcr:primaryType="nt:unstructured">
                                            <additionalLinks
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Get APP Links"
                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                margin="{Boolean}true">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <columns
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                                        margin="{Boolean}true">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <column
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <getapptitle
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Title Text"
                                                                        fieldLabel="Title"
                                                                        name="./getapp/appTitle"/>
                                                                    <getapplink
                                                                        granite:class="lebara-multifield__editor"
                                                                        jcr:primaryType="nt:unstructured"
                                                                        jcr:title="Plans"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                        composite="{Boolean}true"
                                                                        fieldDescription="Add the Get APP section"
                                                                        fieldLabel="Get APP">
                                                                        <granite:data
                                                                            jcr:primaryType="nt:unstructured"
                                                                            maxItems="{Long}2"/>
                                                                        <field
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                            name="./getapp/links">
                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                <column
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                                    <items jcr:primaryType="nt:unstructured">
                                                                                        <label
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                                            fieldDescription="Choose Image"
                                                                                            fieldLabel="Link Image"
                                                                                            name="./label"
                                                                                            rootPath="/content/dam/lebara"/>
                                                                                        <labellink
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                                            fieldDescription="Choose Image link"
                                                                                            fieldLabel="Link"
                                                                                            name="./link"/>
                                                                                    </items>
                                                                                </column>
                                                                            </items>
                                                                        </field>
                                                                    </getapplink>
                                                                </items>
                                                            </column>
                                                        </items>
                                                    </columns>
                                                </items>
                                            </additionalLinks>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </getApp>
                    <followusLinks
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Follow Us Links"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <followustext
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Follow US Text"
                                                fieldLabel="Follow US Text"
                                                name="./followus/followUsText"/>
                                            <followus
                                                granite:class="lebara-multifield__editor"
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Plans"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldDescription="Add the Follow Us icon and link"
                                                fieldLabel="Follow Us">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    maxItems="{Long}5"/>
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./followus/links">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <label
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                    fieldDescription="Choose Image"
                                                                    fieldLabel="Link Image"
                                                                    name="./label"
                                                                    rootPath="/content/dam/lebara"/>
                                                                <labellink
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                    fieldDescription="Choose Image link"
                                                                    fieldLabel="Link"
                                                                    name="./link"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </followus>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </followusLinks>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
