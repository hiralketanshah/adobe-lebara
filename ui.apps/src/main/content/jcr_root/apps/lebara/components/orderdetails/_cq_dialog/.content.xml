<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Order Details"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <productssection
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Products Section"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <selectedproductlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Selected Product Label"
                                                fieldDescription="Selected Product"
                                                name="./selectedProductLabel"/>
                                            <phonenumberlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Phone Number Label"
                                                fieldDescription="Phone Number"
                                                name="./phoneNumberLabel"/>
                                            <missinginfolabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Missing Info Label"
                                                name="./missingInfoLabel"
                                                fieldDescription="Please provide the required missing information."
                                                value="Please provide the required missing information."/>
                                            <freesimlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Free Sim Label"
                                                fieldDescription="Free sim"
                                                name="./freeSimLabel"/>
                                            <topupautorenewlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Auto Renew Label"
                                                fieldDescription="Auto Renew"
                                                name="./topupAutoRenewLabel"/>
                                            <topupautorenewdesc
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Auto Renew Desc"
                                                fieldDescription="Every time your credit reaches â‚¬3 it will be topped up by {0}"
                                                name="./topupAutoRenewDesc"/>
                                            <prepaidautorenewdesc
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Prepaid Auto Renew Desc"
                                                fieldDescription="Your plan will renew every 28 days and you may cancel any time."
                                                name="./prepaidAutoRenewDesc"/>
                                            <postpaidautorenewdesc
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Postpaid Auto Renew Desc"
                                                fieldDescription="Your plan will renew every 28 days and you may cancel any time."
                                                name="./postpaidAutoRenewDesc"/>
                                            <promotionsAppliedLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Promotions Applied Label"
                                                fieldDescription="Promotions Applied"
                                                name="./promotionsAppliedLabel"/>
                                            <appliedLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Applied Label"
                                                fieldDescription="Applied"
                                                name="./appliedLabel"/>
                                            <discountLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Discount Label"
                                                fieldDescription="Discount"
                                                name="./discountLabel"/>
                                            <grandtotallabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Grand Total Label"
                                                fieldDescription="Grand Total"
                                                name="./grandTotalLabel"/>
                                            <totalLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Total Label"
                                                fieldDescription="Total"
                                                name="./totalLabel"/>
                                            <oneTimeActivationFeeLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="One-time activation fee"
                                                fieldLabel="One-time activation fee Label"
                                                name="./yourOrderOneTimeActivationFeeLabel"
                                                value="One-time activation fee"/>
                                            <oneTimeActivationFee
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="15"
                                                fieldLabel="One-time activation fee"
                                                name="./yourOrderOneTimeActivationFee"
                                                value="15"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </productssection>
                    <consentandvouchers
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Consent &amp; vocuher"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <voucherCode
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Voucher Code"
                                                fieldDescription="voucher code"
                                                name="./voucherCodeLabel"/>
                                            <addvouchercodelabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Add Voucher Code Label"
                                                fieldDescription="This field is visible only if the voucher exists"
                                                name="./addVoucherCodeLabel"/>
                                            <applyvoucherlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Apply Voucher Label"
                                                fieldDescription="Apply voucher"
                                                name="./applyVoucherLabel"/>
                                            <entervouchercodelabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Enter Voucher Code Label"
                                                fieldDescription="Enter voucher code"
                                                name="./enterVoucherCodeLabel"/>
                                            <vouchercodeinvalidmessage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Voucher code Invalid Message"
                                                fieldDescription="Voucher code is invalid"
                                                name="./voucherCodeInvalidMessage"/>
                                            <vouchercodeexpiredmessage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Voucher code Expired Message"
                                                fieldDescription="Voucher code has expired"
                                                name="./voucherCodeExpiredMessage"/>
                                            <vouchercodediscountlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Voucher code Discount Label"
                                                fieldDescription="Discount"
                                                name="./voucherCodeDiscountLabel"/>
                                            <consentFragmentPath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="dam/cfm/components/cfpicker"
                                                fieldLabel="Select Consent Fragment"
                                                name="./cfPath"
                                                rootPath="/content/dam/lebara"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </consentandvouchers>
                    <buttonslabel
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Buttons Label"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <paymentmethodlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Payment Method Label"
                                                fieldDescription="Select Payment Method"
												value="Select Payment Method"
                                                name="./paymentMethodLabel"/>
                                            <paymentbuttonlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Payment Button Label"
                                                fieldDescription="Proceed to payment"
                                                name="./paymentButtonLabel"/>
                                            <ctaContinueLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="CTA Continue Label"
                                                fieldDescription="Continue"
                                                name="./ctaContinueLabel"/>
                                            <removelabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Remove Label"
                                                fieldDescription="Remove"
                                                name="./removeLabel"/>
                                            <showdetailslabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Show Details Label"
                                                fieldDescription="SHow Details"
                                                name="./showDetailsLabel"/>
                                            <viewplanslabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="View Plans Label"
                                                fieldDescription="View Plans"
                                                name="./viewPlansLabel"/>
                                            <addToCartLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Add to Cart Label"
                                                fieldDescription="Add to cart"
                                                name="./addToCartLabel"/>
                                            <buyPlanLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Buy Plan Label"
                                                fieldDescription="Buy Plan"
                                                name="./buyPlanLabel"/>
                                            <changePlanLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Change Plan Label"
                                                fieldDescription="Change Plan"
                                                name="./changePlanLabel"/>
                                            <changeAddonLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Change Addon Label"
                                                fieldDescription="Change Addon"
                                                name="./changeAddonLabel"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </buttonslabel>
                    <deletecartitem
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Delete Cart Item Popup"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <deletecartitemtitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Configure with a placeholder (ex: Remove {0})"
                                                fieldLabel="Title"
                                                name="./deleteCartItemTitle"/>
                                            <deletecartitemdesc
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Configure with a placeholder (ex: Are you sure you wanna remove{0} from cart?)"
                                                fieldLabel="Delete cart item desc"
                                                name="./deleteCartItemDesc"/>
                                            <deletecartitemyesbuttonlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Delete Cart Item Yes Button Label"
                                                fieldDescription="Yes"
                                                name="./deleteCartItemYesButtonLabel"/>
                                            <deletecartitemnobuttonlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Delete Cart Item No Button Label"
                                                fieldDescription="No"
                                                name="./deleteCartItemNoButtonLabel"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </deletecartitem>
                    <topuplabels
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Top Up Labels"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <topupcreditlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Top Up Credit Label"
                                                fieldDescription="Top up credit"
                                                name="./topUpCreditLabel"/>
                                            <topuprecommendedlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Top Up Recommended Label"
                                                fieldDescription="Recommended top up"
                                                name="./topUpRecommendedLabel"/>
                                            <topupcaplabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Top Up Cap Label"
                                                fieldDescription="Top up cap"
                                                name="./topUpCapLabel"/>
                                            <topupcapdesc
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Top Up Cap Desc"
                                                fieldDescription="Per Month Exceed limit for Auto Top Up"
                                                name="./topUpCapDesc"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </topuplabels>
                    <changeplanpopup
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Change Plan Popup"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <planstitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Plans Title"
                                                fieldDescription="Plans"
                                                name="./plansTitle"
                                                value="Plans"/>
                                            <addonstitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Addons Title"
                                                fieldDescription="Addons"
                                                name="./addonsTitle"
                                                value="Addons"/>
                                            <selectplanlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Select Plan Label"
                                                fieldDescription="Select Plan"
                                                name="./selectPlanLabel"/>
                                            <hidechangeplan
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                name="./hideChangePlan"
                                                text="Hide Change Plan?"
                                                uncheckedValue="{Boolean}false"
                                                value="{Boolean}true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </changeplanpopup>
                    <personaldetails
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Personal Details"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <personaldetailslabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Personal details Label"
                                                name="./personalDetailsLabel"
                                                fieldDescription="Your personal details"
                                                value="Your personal details"/>
                                            <namelabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="NameTitle"
                                                name="./nameLabel"
                                                fieldDescription="Your name:"
                                                value="Your name:"/>
                                            <emaillabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Email Label"
                                                name="./emailLabel"
                                                fieldDescription="Your email address:"
                                                value="Your email address:"/>
                                            <mobilenumberlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Mobile Number Label"
                                                name="./mobileNumberLabel"
                                                fieldDescription="Your mobile number:"
                                                value="Your mobile number:"/>
                                            <shippingaddresslabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Shipping address Label"
                                                name="./shippingAddressLabel"
                                                fieldDescription="Your shipping address:"
                                                value="Your shipping address:"/>
                                            <editlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Edit Label"
                                                name="./editLabel"
                                                fieldDescription="Edit"
                                                value="Edit"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </personaldetails>
                    <contractSummary
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Contract Summary"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <contractSummarylabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Contract Summary Label"
                                                    name="./contractSummary"
                                                    fieldDescription="Contract Summary Label"
                                                    value="CONTRACT SUMMARY"/>
                                            <contractsummarypdfrootpath
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Root path for Contract Summary PDF"
                                                    name="./contractSummaryPdfRootPath"
                                                    rootPath="/content/dam/lebara"/>
                                            <pdfdownloadlabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="PDF Download Label"
                                                    name="./pdfDownloadLabel"
                                                    fieldDescription="PDF Download Label"
                                                    value="Download"/>
                                            <pdfcloselabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="PDF Close Label"
                                                    name="./pdfCloseLabel"
                                                    fieldDescription="PDF Close Label"
                                                    value="Close"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </contractSummary>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
