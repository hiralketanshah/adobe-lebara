<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Dashboard Prepaid Plans"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs"
                    maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <commonColumn
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Dashboard Prepaid Plans"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <prepaidPlanHeading
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Prepaid Plan heading Text"
                                    value="Your Plans"
                                    fieldLabel="Prepaid Plan heading"
                                    name="./title"/>
                            <remainderText
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Left Of Text"
                                    value="Left Of"
                                    fieldLabel="Left Of Text"
                                    name="./leftOfText"/>
                            <manageLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Plan Management Label"
                                    value="Manage"
                                    fieldLabel="Plan Management Label"
                                    name="./manageLabel"/>
                            <prepaidPlanTabs
                                    granite:class="lebara-multifield__editor"
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Tabs"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                    composite="{Boolean}true"
                                    fieldDescription="Tabs Name"
                                    fieldLabel="Tabs Name">
                                <granite:data
                                        jcr:primaryType="nt:unstructured"
                                        maxItems="{Long}4"/>
                                <field
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        name="./tabs">
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <tabsLabel
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Tab Name"
                                                        fieldLabel="Tab Name"
                                                        name="./tabsName"/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </prepaidPlanTabs>
                        </items>
                    </commonColumn>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
