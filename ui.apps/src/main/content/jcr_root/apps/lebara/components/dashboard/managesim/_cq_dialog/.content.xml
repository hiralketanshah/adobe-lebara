<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Manage Sim"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[lebara.touchui]">
    <content
        granite:class="cmp-navigation__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <heading
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Generic Config"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Title"
                                                name="./title"
                                                value="Attach your SIM"/>
                                            <linkedSimDesc
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldDescription="Add a placeholder {0} which will be replaced by number of sim linked dynamically"
                                                fieldLabel="Linked Sim Description"
                                                name="./linkedSimDesc"
                                                value="There is {0} SIM linked to your MyLebara"/>
                                            <attachNewSim
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Attach New Sim Label"
                                                name="./attachNewSim"
                                                value="Attach new sim"/>
                                            <linkSimLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Link Sim Label"
                                                name="./linkSimLabel"
                                                value="Please complete the below to link your SIM."/>
                                            <noSimLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="No Sim Label"
                                                name="./noSimLabel"
                                                value="I don't have a sim"/>
                                            <activeMonthPlanLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Active Month Plan Label"
                                                name="./activeMonthPlanLabel"
                                                value="This number have active month plan"/>
                                            <noLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="No Label"
                                                name="./noLabel"
                                                value="No"/>
                                            <mobilePlaceholderLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Mobile Placeholder Label"
                                                name="./mobilePlaceholderLabel"
                                                value="Lebara Mobile Number"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </heading>
                    <delink
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Delink Sim Config"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <delinkLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Delink Sim Label"
                                                name="./delinkLabel"
                                                value="DELINK SIM"/>
                                            <delinkContinueLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Delink Continue Label"
                                                name="./ctaContinueLabel"
                                                value="CONTINUE"/>
                                            <delinkConfirmationMessage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldDescription="Enter Delink sim confirmation message, Add a placeholder {0} which will be replaced by dynamic sim number"
                                                fieldLabel="Delink Confirmation Message"
                                                name="./delinkConfirmationMsg"
                                                value="Are you sure you want to Unlink {0} from your account"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </delink>
                    <delinkconfirmationdialog
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Delink Confirmation Dialog"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <confirmationEmailLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Confirmation Email Label"
                                                name="./confirmationEmailLabel"
                                                value="An Confirmation email has been sent to your registered email Id"/>
                                            <followLinkLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Follow Link Label"
                                                name="./followLinkLabel"
                                                value="Please follow the link we have sent to the email address"/>
                                            <mailNotReceivedLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="If you havent received the email yet"
                                                fieldLabel="Mail Not Received Label"
                                                name="./mailNotReceivedLabel"
                                                value=" If you havent received the email yet"/>
                                            <clickHereLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Click Here Label"
                                                name="./clickHereLabel"
                                                value="Click Here"/>
                                            <toResendLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="To Resend Label"
                                                name="./toResendLabel"
                                                value="to resend"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </delinkconfirmationdialog>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
