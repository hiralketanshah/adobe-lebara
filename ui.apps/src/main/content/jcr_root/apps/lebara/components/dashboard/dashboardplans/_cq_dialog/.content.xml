<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Dashboard Plans Component"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <messages
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Generic Labels"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Title"
                                                value="My Plans"
                                                name="./title"/>
                                            <buttonLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Manage"
                                                value="Manage"
                                                name="./buttonLabel"/>
                                            <autoRenewLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Auto renew"
                                                value="Auto renew"
                                                name="./autoRenewLabel"/>
                                            <renewalLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Renewal"
                                                value="Renewal"
                                                name="./renewalLabel"/>
                                            <autoRenewDesc
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="AutoRenew Description"
                                                name="./autoRenewDesc"/>
                                            <autoRenewTurnOnLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                value="AutoRenew TurnOn Label"
                                                fieldLabel="Turn on the auto renewal service"
                                                name="./autoRenewTurnOnLabel"/>
                                            <buttonText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="buy it again"
                                                    value="buy it again"
                                                    name="./buttonText"/>
                                            <cancelLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Cancel Request"
                                                    value="Cancel Request"
                                                    name="./cancelLabel"/>
                                            <planChangeMessage
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Plan Change Message"
                                                    name="./planChangeMessage"/>
                                            <manageLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Manage"
                                                    value="Manage"
                                                    name="./manageLabel"/>
                                            <showPlansWithProgress
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                name="./showPlansWithProgress"
                                                text="Show Plans with progress?"
                                                uncheckedValue="{Boolean}false"
                                                value="{Boolean}true"/>
                                            <dataValue
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Data Value"
                                                value="MB"
                                                fieldDescription="./dataValue"/>
                                            <minutesValue
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="minutesValue"
                                                fieldDescription="MIN"
                                                name="./minutesValue"/>
                                            <smsValue
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="SMS Value"
                                                fieldDescription="SMS"
                                                name="./smsValue"/>
                                            <leftOfLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Left Of Label Label"
                                                fieldDescription="0 left of 1"
                                                name="./leftOfLabel"/>
                                            <validLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Valid to Label"
                                                fieldDescription="valid to"
                                                name="./validLabel"/>
                                            <offlinePlanRenewalMessage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Offline Plan Renewal Message"
                                                fieldDescription="Renews in 28 days from top up balance. You can turn it off via customer care"
                                                name="./offlinePlanRenewalMessage"/>
                                            <dataTabName
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Data Tab Name"
                                                fieldDescription="Label that will appear on Data Tab e.g. Data"
                                                value="Data"
                                                name="./dataTabName"/>
                                            <minTabName
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Minutes Tab Name"
                                                value="Min"
                                                fieldDescription="Label that will appear on Minutes Tab e.g. Min"
                                                name="./minTabName"/>
                                            <smsTabName
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="SMS Tab Name"
                                                value="SMS"
                                                fieldDescription="Label that will appear on SMS Tab e.g. SMS"
                                                name="./smsTabName"/>
                                            <internationalMinTabName
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="International Minutes Tab Name"
                                                value="International min"
                                                fieldDescription="Label that will appear on International Minutes Tab e.g. International min"
                                                name="./internationalMinTabName"/>
                                            <usedLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Consumption Label for Unlimited Plan"
                                                fieldDescription="Label that will appear for showing consumption for Unlimited Plan e.g. {0} {1} Used"
                                                name="./usedLabel"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </messages>
                    <cancelChangePlan
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Cancel Change Plan popup"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <cancelChangePlanHeading
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Cancel Change Plan Heading"
                                    name="./cancelChangePlanHeading"
                                    value="Cancel Plan Change"/>
                            <cancelChangePlanSubHeading
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Cancel Change Plan SubHeading"
                                    fieldLabel="Cancel Change Plan SubHeading"
                                    value="Are you sure you would like to cancel the switch?"
                                    name="./cancelChangePlanSubHeading"/>
                            <cancelChangePlanCheckboxTop
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                    fieldLabel="Cancel Change plan First Checkbox"
                                    value="I confirm the change and agree the Terms and conditions"
                                    name="./cancelChangePlanCheckboxTop"
                                    useFixedInlineToolbar="{Boolean}true">
                                <rtePlugins jcr:primaryType="nt:unstructured">
                                    <format
                                            jcr:primaryType="nt:unstructured"
                                            features="bold,italic"/>
                                    <justify
                                            jcr:primaryType="nt:unstructured"
                                            features="-"/>
                                    <links
                                            jcr:primaryType="nt:unstructured"
                                            features="modifylink,unlink"/>
                                    <lists
                                            jcr:primaryType="nt:unstructured"
                                            features="*"/>
                                    <paraformat
                                            jcr:primaryType="nt:unstructured"
                                            features="*">
                                        <formats jcr:primaryType="nt:unstructured">
                                            <default_p
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Paragraph"
                                                    tag="p"/>
                                            <default_h1
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Heading 1"
                                                    tag="h1"/>
                                            <default_h2
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Heading 2"
                                                    tag="h2"/>
                                            <default_h3
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Heading 3"
                                                    tag="h3"/>
                                            <default_h4
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Heading 4"
                                                    tag="h4"/>
                                            <default_h5
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Heading 5"
                                                    tag="h5"/>
                                            <default_h6
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Heading 6"
                                                    tag="h6"/>
                                            <default_blockquote
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Quote"
                                                    tag="blockquote"/>
                                            <default_pre
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Preformatted"
                                                    tag="pre"/>
                                        </formats>
                                    </paraformat>
                                    <tracklinks
                                            jcr:primaryType="nt:unstructured"
                                            features="*"/>
                                </rtePlugins>
                                <uiSettings jcr:primaryType="nt:unstructured">
                                    <cui jcr:primaryType="nt:unstructured">
                                        <inline
                                                jcr:primaryType="nt:unstructured"
                                                toolbar="[format#bold,format#italic,format#underline,#justify,#lists,links#modifylink,links#unlink,#paraformat]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <justify
                                                        jcr:primaryType="nt:unstructured"
                                                        items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                        ref="justify"/>
                                                <lists
                                                        jcr:primaryType="nt:unstructured"
                                                        items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                        ref="lists"/>
                                                <paraformat
                                                        jcr:primaryType="nt:unstructured"
                                                        items="paraformat:getFormats:paraformat-pulldown"
                                                        ref="paraformat"/>
                                            </popovers>
                                        </inline>
                                        <dialogFullScreen
                                                jcr:primaryType="nt:unstructured"
                                                toolbar="[format#bold,format#italic,format#underline,justify#justifyleft,justify#justifycenter,justify#justifyright,lists#unordered,lists#ordered,lists#outdent,lists#indent,links#modifylink,links#unlink,table#createoredit,#paraformat,image#imageProps]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <paraformat
                                                        jcr:primaryType="nt:unstructured"
                                                        items="paraformat:getFormats:paraformat-pulldown"
                                                        ref="paraformat"/>
                                            </popovers>
                                        </dialogFullScreen>
                                    </cui>
                                </uiSettings>
                            </cancelChangePlanCheckboxTop>
                            <cancelChangePlanCheckboxBottom
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                    name="./cancelChangePlanCheckboxBottom"
                                    fieldLabel="Cancel Change plan Second Checkbox"
                                    value="I Confirm that i am the holder of the plan"
                                    useFixedInlineToolbar="{Boolean}true">
                                <rtePlugins jcr:primaryType="nt:unstructured">
                                    <format
                                            jcr:primaryType="nt:unstructured"
                                            features="bold,italic"/>
                                    <justify
                                            jcr:primaryType="nt:unstructured"
                                            features="-"/>
                                    <links
                                            jcr:primaryType="nt:unstructured"
                                            features="modifylink,unlink"/>
                                    <lists
                                            jcr:primaryType="nt:unstructured"
                                            features="*"/>
                                    <paraformat
                                            jcr:primaryType="nt:unstructured"
                                            features="*">
                                        <formats jcr:primaryType="nt:unstructured">
                                            <default_p
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Paragraph"
                                                    tag="p"/>
                                            <default_h1
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Heading 1"
                                                    tag="h1"/>
                                            <default_h2
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Heading 2"
                                                    tag="h2"/>
                                            <default_h3
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Heading 3"
                                                    tag="h3"/>
                                            <default_h4
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Heading 4"
                                                    tag="h4"/>
                                            <default_h5
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Heading 5"
                                                    tag="h5"/>
                                            <default_h6
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Heading 6"
                                                    tag="h6"/>
                                            <default_blockquote
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Quote"
                                                    tag="blockquote"/>
                                            <default_pre
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Preformatted"
                                                    tag="pre"/>
                                        </formats>
                                    </paraformat>
                                    <tracklinks
                                            jcr:primaryType="nt:unstructured"
                                            features="*"/>
                                </rtePlugins>
                                <uiSettings jcr:primaryType="nt:unstructured">
                                    <cui jcr:primaryType="nt:unstructured">
                                        <inline
                                                jcr:primaryType="nt:unstructured"
                                                toolbar="[format#bold,format#italic,format#underline,#justify,#lists,links#modifylink,links#unlink,#paraformat]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <justify
                                                        jcr:primaryType="nt:unstructured"
                                                        items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                        ref="justify"/>
                                                <lists
                                                        jcr:primaryType="nt:unstructured"
                                                        items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                        ref="lists"/>
                                                <paraformat
                                                        jcr:primaryType="nt:unstructured"
                                                        items="paraformat:getFormats:paraformat-pulldown"
                                                        ref="paraformat"/>
                                            </popovers>
                                        </inline>
                                        <dialogFullScreen
                                                jcr:primaryType="nt:unstructured"
                                                toolbar="[format#bold,format#italic,format#underline,justify#justifyleft,justify#justifycenter,justify#justifyright,lists#unordered,lists#ordered,lists#outdent,lists#indent,links#modifylink,links#unlink,table#createoredit,#paraformat,image#imageProps]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <paraformat
                                                        jcr:primaryType="nt:unstructured"
                                                        items="paraformat:getFormats:paraformat-pulldown"
                                                        ref="paraformat"/>
                                            </popovers>
                                        </dialogFullScreen>
                                    </cui>
                                </uiSettings>
                            </cancelChangePlanCheckboxBottom>
                            <cancelChangePlanConfirmLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Cancel Change Plan Confirm Label"
                                    fieldDescription="Cancel Change Plan Confirm Label"
                                    name="./cancelChangePlanConfirmLabel"
                                    value="Confirm"/>
                            <cancelChangePlanCancelLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Cancel Change Plan Cancel Label"
                                    value="Cancel"
                                    fieldLabel="Cancel Change Plan Cancel Label"
                                    name="./cancelChangePlanCancelLabel"/>
                        </items>
                    </cancelChangePlan>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
