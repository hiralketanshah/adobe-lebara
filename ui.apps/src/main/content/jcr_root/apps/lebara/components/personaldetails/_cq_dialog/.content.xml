<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Personal Details Component"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs"
                    maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <column1
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Name Section"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <yourPersonalDetailsLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Your personal details Label"
                                    value="Your personal details"
                                    fieldDescription="Your personal details"
                                    name="./yourPersonalDetailsLabel"/>
                            <firstNameLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="First Name Label"
                                    value="First Name"
                                    fieldDescription="First Name"
                                    name="./firstNameLabel"/>
                            <firstNamePlaceholder
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="First Name Placeholder"
                                    value="Enter Your First Name"
                                    fieldDescription="Enter Your First Name"
                                    name="./firstNamePlaceholder"/>
                            <lastNameLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Last Name Label"
                                    value="Last Name"
                                    name="./lastNameLabel"/>
                            <lastNamePlaceholder
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Last Name Placeholder"
                                    value="Enter Your First Name"
                                    fieldDescription="Enter Your First Name"
                                    name="./lastNamePlaceholder"/>
                            <emailAddressLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Email Address Label"
                                    value="Email Address"
                                    fieldDescription="Email Address"
                                    name="./emailAddressLabel"/>
                            <emailAddressLabelPlaceholder
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Email Address Placeholder"
                                    value="Enter Your Email Address"
                                    fieldDescription="Enter Your Email Address"
                                    name="./emailAddressPlaceholder"/>
                        </items>
                    </column1>
                    <column2
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Address Section"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <keyInAddress
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Key in your address manually"
                                    value="Key in your address manually"
                                    fieldDescription="Key in your address manually"
                                    name="./keyInAddress"/>
                            <address
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Address label"
                                    value="Address"
                                    fieldDescription="Address"
                                    name="./addressLabel"/>
                            <addressKeyInText
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Address key in message"
                                    value="Key in your postal code and find your address"
                                    fieldDescription="Key in your postal code and find your address"
                                    name="./addressKeyInText"/>
                            <postalcodePlaceholder
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Search For Postal Code placeholder text"
                                    value="Search For Postal Code"
                                    fieldDescription="Search For Postal Code"
                                    name="./postalcodePlaceholder"/>
                            <enterAddressManually
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Enter Address Manually label"
                                    value="Enter Address Manually?"
                                    fieldDescription="Enter Address Manually?"
                                    name="./enterAddressManually"/>
                            <changeAddressLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Change Address label"
                                    value="Change Address"
                                    fieldDescription="Change Address"
                                    name="./changeAddressLabel"/>
                            <streetLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Street Label"
                                    value="Street"
                                    name="./streetLabel"/>
                            <streetPlaceholder
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Street Placeholder"
                                    value="Enter Your Street Name"
                                    fieldDescription="Enter Your Street Name"
                                    name="./streetPlaceholder"/>
                            <houseNumberLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="House Number Label"
                                    value="House Number"
                                    name="./houseNumberLabel"/>
                            <houseNumberPlaceholder
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="House Number Placeholder"
                                    value="Enter Your House Number"
                                    name="./houseNumberPlaceholder"/>
                            <zipCodeLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Zip Code Label"
                                    value="Zip Code"
                                    fieldDescription="Zip Code"
                                    name="./zipCodeLabel"/>
                            <zipCodePlaceholder
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Zip Code Placeholder"
                                    value="Enter Your Zip Code"
                                    fieldDescription="Enter Your Zip Code"
                                    name="./zipCodePlaceholder"/>
                            <cityLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="city Label"
                                    value="City"
                                    name="./cityLabel"/>
                            <cityPlaceholder
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="City Placeholder"
                                    value="Enter Your City"
                                    name="./cityPlaceholder"/>
                            <saveAddress
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Save address for future label"
                                    value="Save address for future"
                                    fieldDescription="Save address for future"
                                    name="./saveAddress"/>
                        </items>
                    </column2>
                    <column3
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Order Details CTA"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <cityFragmentPath
                                    cq:showOnCreate="{Boolean}true"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="dam/cfm/components/cfpicker"
                                    fieldLabel="City Content Frament path"
                                    fieldDescription="City Content Frament path"
                                    name="./cfPath"
                                    required="{Boolean}true"
                                    rootPath="/content/dam/lebara"/>
                            <orderDetailsCta
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Order Details Cta label"
                                    value="CONTINUE TO ORDER DETAILS"
                                    fieldDescription="CONTINUE TO ORDER DETAILS"
                                    name="./orderDetailsCta"/>
                            <paymentcta
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="PROCEED TO PAYMENT"
                                fieldLabel="Payment CTA"
                                name="./paymentCta"/>
                        </items>
                    </column3>
                    <column4
                            jcr:primaryType="nt:unstructured"
                            jcr:title="First and last Name Error Messages"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <firstNameErrorMax
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="First Name Error message on character limit"
                                    value="Must be 50 characters or less"
                                    name="./firstNameErrorMax"/>
                            <firstNameErrorRequired
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="First Name Error message on Empty Names"
                                    value="Please enter a valid First Name , First name cannot be empty"
                                    name="./firstNameErrorRequired"/>
                            <firstNameErrorPattern
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="First Name Error message on Invalid characters"
                                    value="The First name cannot contain any special characters ($, @, #, !, %. * ^() ?)"
                                    name="./firstNameErrorPattern"/>
                            <lastNameErrorMax
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Last Name Error message on character limit"
                                    value="Must be 75 characters or less"
                                    fieldDescription="Must be 75 characters or less"
                                    name="./lastNameErrorMax"/>
                            <lastNameErrorRequired
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Last Name Error message on Empty Names"
                                    value="Please enter a valid Last Name , Last name cannot be empty"
                                    name="./lastNameErrorRequired"/>
                            <lastNameErrorPattern
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Last Name Error message on Invalid characters"
                                    value="The Last name cannot contain any special characters ($, @, #, !, %. * ^() ?)"
                                    name="./lastNameErrorPattern"/>
                        </items>
                    </column4>
                    <column5
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Email and Address Error Messages"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <emailErrorMax
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Email Error message on character limit"
                                    value="Must be 100 characters or less"
                                    fieldDescription="Must be 100 characters or less"
                                    name="./emailErrorMax"/>
                            <emailErrorRequired
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Email Error message on Empty Email"
                                    value="The email address field cannot be empty"
                                    fieldDescription="The email address field cannot be empty"
                                    name="./emailErrorRequired"/>
                            <emailErrorPattern
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Email Error message on Invalid characters"
                                    value="Please enter a valid email address. For example username@domain.com"
                                    fieldDescription="Please enter a valid email address. For example username@domain.com"
                                    name="./emailErrorPattern"/>
                            <existingUserErrorMsg
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Existing user error message"
                                    value="This email already has an account attached. Please use a different email"
                                    fieldDescription="This email already has an account attached. Please use a different email"
                                    name="./existingUserErrorMsg"/>
                            <addressErrorRequired
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Address Error message on Empty Address"
                                    value="Address is required"
                                    fieldDescription="Address is required"
                                    name="./addressErrorRequired"/>
                            <streetLabelErrorMax
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Street Name Error message on character limit"
                                    value="Must be 250 characters or less"
                                    name="./streetLabelErrorMax"/>
                            <streetLabelErrorRequired
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Street Name Error message on Empty Street Names"
                                    value="Street name is required"
                                    name="./streetLabelErrorRequired"/>
                            <streetLabelErrorPattern
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Street Name Error message on Invalid characters"
                                    value="Invalid street name"
                                    name="./streetLabelErrorPattern"/>
                            <houseNumberErrorMax
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="House Number Error message on character limit"
                                    value="Must be 20 characters or less"
                                    name="./houseNumberErrorMax"/>
                            <houseNumberErrorRequired
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="House Number Error message on Empty Street Names"
                                    value="House number is required"
                                    name="./houseNumberErrorRequired"/>
                            <cityErrorMax
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Town/City Error message on character limit"
                                    value="Must be 20 characters or less"
                                    name="./cityErrorMax"/>
                            <cityErrorRequired
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Town/City Error message on Empty Street Names"
                                    value="Town/City is required"
                                    fieldDescription="Town/City is required"
                                    name="./cityErrorRequired"/>
                            <houseNumberErrorPattern
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="House Number Error message on Invalid characters"
                                    value="Invalid house number"
                                    fieldDescription="Invalid house number"
                                    name="./houseNumberErrorPattern"/>
                            <zipCodeErrorMin
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="zipCode Error message on Min character limit"
                                    value="Must be 5 characters or more"
                                    name="./zipCodeErrorMin"/>
                            <zipCodeErrorRequired
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="zipCode Error message on Empty zipCode values"
                                    value="Zip Code is required"
                                    fieldDescription="Zip Code is required"
                                    name="./zipCodeErrorRequired"/>
                            <zipCodeErrorPattern
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="zipCode Error message on Invalid characters"
                                    value="Invalid Zip Code"
                                    fieldDescription="Invalid Zip Code"
                                    name="./zipCodeErrorPattern"/>
                            <zipCodeErrorMax
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="zipCode Error message on Max character limit"
                                    value="Must be 10 characters or less"
                                    name="./zipCodeErrorMax"/>
                        </items>
                    </column5>
                    <column6
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Password Messages"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <passwordfield
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Password"
                                fieldLabel="Password Field"
                                name="./passwordField"/>
                            <confirmpasswordfield
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Confirm Password"
                                fieldLabel="Confirm Password Field"
                                name="./confirmPasswordField"/>
                            <enterpasswordlabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter a password"
                                fieldLabel="Enter Password Label"
                                name="./enterPasswordLabel"/>
                            <confirmpasswordlabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter a confirm password"
                                fieldLabel="Confirm Password Label"
                                name="./confirmPasswordLabel"/>
                            <minimumcharacterslabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Minimum 8 characters"
                                fieldLabel="Minimum Characters Label"
                                name="./minimumCharactersLabel"/>
                            <samepasswordlabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Both password need to be the same"
                                fieldLabel="Same Password Label"
                                name="./samePasswordLabel"/>
                            <showpasswordlabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Show"
                                fieldLabel="Show Password Label"
                                name="./showPasswordLabel"/>
                            <hidepasswordlabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Hide"
                                fieldLabel="Hide Password Label"
                                name="./hidePasswordLabel"/>
                        </items>
                    </column6>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
