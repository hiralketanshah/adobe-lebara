<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Address Change OTP"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[lebara.touchui]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <verificationlabels
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Verify to continue"
                                                fieldLabel="Heading"
                                                name="./heading"
                                                value="Verify to continue"/>
                                            <subHeading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldDescription="To continue change of address get the verification address and put it below/"
                                                fieldLabel="Sub Heading"
                                                name="./subHeading"/>
                                            <enterCodeLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the code sent to {0}"
                                                fieldLabel="Enter code Label"
                                                name="./enterCodeLabel"
                                                value="Enter the code sent to {0}"/>
                                            <simCardLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Sim card number"
                                                fieldLabel="Sim card number Label"
                                                name="./simCardLabel"
                                                value="Sim card number"/>
                                            <verifyCodeLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Verification Code"
                                                fieldLabel="Verification Code Label"
                                                name="./verifyCodeLabel"
                                                value="Verification Code"/>
                                            <verifyCodePlaceholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Verification code"
                                                fieldLabel="Verification Code Label"
                                                name="./verifyCodePlaceholder"
                                                value="Enter Verification code"/>
                                            <resendVerifyCodeInLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Resend verification code in"
                                                fieldLabel="Resend verification code in Label"
                                                name="./resendVerifyCodeInLabel"
                                                value="Resend verification code in"/>
                                            <resendVerifyCodeLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Resend verification code"
                                                fieldLabel="Resend verification code Label"
                                                name="./resendVerifyCodeLabel"
                                                value="Resend verification code"/>
                                            <otpSentSuccessMessage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="OTP is sent successfully."
                                                fieldLabel="OTP Sent Success Message"
                                                name="./otpSentSuccessMessage"
                                                value="OTP is sent successfully."/>
                                            <otpExpiredErrorMessage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Expired OTP Code Error Message"
                                                fieldLabel="Expired OTP code label"
                                                name="./otpExpiredErrorMessage"
                                                value="Expired OTP Code"/>
                                            <otpInvalidErrorMessage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Invalid OTP Code"
                                                fieldLabel="Invalid OTP code Error Message"
                                                name="./otpInvalidErrorMessage"
                                                value="Invalid OTP Code"/>
                                            <exceededLimitLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Exceeded the limit"
                                                fieldLabel="Exceeded Limit Label"
                                                name="./exceededLimitLabel"
                                                value="Exceeded the limit"/>
                                            <exceededLimitDesc
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldDescription="You have exceeded the code limit entry in ( 3 times in 60 secs). Verify with OTAC is blocked to 30 mins"
                                                fieldLabel="Exceeded Limit Description"
                                                name="./exceededLimitDesc"/>
                                            <requiredErrorMessage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Required"
                                                fieldLabel="Required Error Message"
                                                name="./requiredErrorMessage"
                                                value="Required"/>
                                            <digitsErrorMessage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Must be 5 digits"
                                                fieldLabel="Digits Error Message"
                                                name="./digitsErrorMessage"
                                                value="Must be 5 digits"/>
                                            <ctaContinueLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Continue"
                                                fieldLabel="Continue Button Label"
                                                name="./ctaContinueLabel"
                                                value="Continue"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </verificationlabels>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
