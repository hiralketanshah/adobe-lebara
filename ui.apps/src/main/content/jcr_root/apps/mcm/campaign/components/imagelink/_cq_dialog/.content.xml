                                                <format
                                                    jcr:primaryType="nt:unstructured"
                                                    icon="text"
                                                    items="[format#bold,format#italic,format#underline,subsuperscript#subscript,subsuperscript#superscript]"
                                                    ref="format"/>
                                                <justify
                                                    jcr:primaryType="nt:unstructured"
                                                    icon="text"
                                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                    ref="justify"/>
