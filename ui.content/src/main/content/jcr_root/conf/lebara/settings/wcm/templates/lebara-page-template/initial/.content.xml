<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/lebara/settings/wcm/templates/lebara-page-template"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="lebara/components/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <experiencefragment-header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="lebara/components/experiencefragment"
                fragmentVariationPath="/content/experience-fragments/lebara/de/en/site/header/master"/>
            <breadcrumb
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="lebara/components/breadcrumb"
                    editable="{Boolean}true"/>
            <responsivegrid
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid"/>
            <experiencefragment-footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="lebara/components/experiencefragment"
                fragmentVariationPath="/content/experience-fragments/lebara/de/en/site/footer/master"/>
        </root>
    </jcr:content>
</jcr:root>
